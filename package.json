{
  "name": "webbluetooth",
  "version": "2.1.0",
  "description": "Node.js implementation of the Web Bluetooth Specification",
  "homepage": "https://github.com/thegecko/webbluetooth",
  "author": "Rob Moran <rob@thegecko.org>",
  "license": "MIT",
  "main": "dist/webbluetooth.umd.js",
  "module": "dist/webbluetooth.esm.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/thegecko/webbluetooth.git"
  },
  "keywords": [
    "web-bluetooth",
    "ble",
    "bluetooth"
  ],
  "engines": {
    "node": ">=8.14.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "clean": "git clean -f -x ./dist ./docs",
    "build": "webpack --mode production && yarn lint && yarn docs",
    "watch": "webpack --mode=development -w",
    "lint": "eslint . --ext .ts,.tsx",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "ts-loader": "^9.3.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.2.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@abandonware/noble": "1.9.2-15",
    "@types/node": "^8.10.60",
    "@types/web-bluetooth": "^0.0.5"
  }
}
